{
  "title": "Personal keys",
  "rules": [
    {
      "description": "Firefox redo to ctrl + Y",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            },
            "key_code": "y"
          },
          "to": [
            {
              "modifiers": [
                "left_control"
              ],
              "key_code": "z"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org\\.mozilla\\.firefox$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
